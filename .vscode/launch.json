    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--type", "recommend-config",
                "--task_id", "q1",
                "--mode", "multi",
                "--workload", "TPCH",
                "--data_size", "100",
                "--model", "tbcnn",
            ],
            "justMyCode": true
        }
    ]
}